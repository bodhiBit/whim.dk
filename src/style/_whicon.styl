header.docked
  cursor                        move
  float                         left
  width                         480px

nav.docked
  /* -webkit-column-width          114px
  -moz-column-width             114px
  column-width                  114px */
  float                         right
  width                         140px
  height                        100%

footer.docked
  position                      absolute
  text-align                    left
  bottom                        1em
  left                          1em
  height                        150px

[am-icon]
  display                       inline-block
  font-size                     .8em
  line-height                   1em
  padding                       .4em .2em
  text-align                    center
  vertical-align                top
  width                         96px
  
  a
    text-decoration             none
  
  img::after
    display                     block

nav [am-icon], footer [am-icon]
  background-image              url("paper_texture.png")
  box-shadow                    0 0 5px 0 #0003
  margin                        .8em
  